  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build --webpack --webpackPath webpack.config.js && npm run sentry:sourcemaps",
    "build:dev": "nest build --webpack --webpackPath webpack.config.js",
    "dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider TYPEORM_LOGGING=true nest build --webpack --webpackPath webpack-hmr.config.js",
    "dev:debug": "nest start --debug --watch",
    "prod:start": "node --max-old-space-size=8192 dist/main",
    "tsc": "tsc",
    "test": "yarn test:unit && yarn test:integration",
    "test:unit": "jest -i --forceExit --coverage",
    "test:update-snapshots": "yarn test:update-unit-snapshots && yarn test:update-integration-snapshots",
    "test:update-unit-snapshots": "jest -i --forceExit --coverage --updateSnapshot",
    "test:update-integration-snapshots": "jest -i --forceExit --updateSnapshot --config ./test/jest-integration.json",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:integration": "jest --config ./test/jest-integration.json -i",
    "dev:db:reset": "yarn typeorm schema:drop",
    "migration:generate": "yarn schema:drop && yarn migration:run && yarn typeorm migration:generate",
    "migration:run": "yarn typeorm migration:run -d ./typeORMCLI.config.ts",
    "schema:drop": "yarn typeorm schema:drop -d ./typeORMCLI.config.ts",
    "typeorm": "cross-env TYPEORM_CLI=true ts-node -r tsconfig-paths/register ../../node_modules/typeorm/cli.js",
    "cli": "nestjs-command",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org university-of-british-colum-p0 --project helpme-nestjs ./dist && sentry-cli --url https://sentry.io/ sourcemaps upload --org university-of-british-colum-p0 --project helpme-nestjs ./dist"
  },
